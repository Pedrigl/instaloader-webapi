# Copy this to .env and fill in secrets locally. Do NOT commit .env to git.

# Database (Postgres asyncpg URL)
DATABASE_URL=postgresql+asyncpg://user:password@localhost/instaloader

# OpenAI
OPENAI_API_KEY=sk-...
OPENAI_MODEL=gpt-4o-mini

# Worker targets
WORKER_TARGETS=store_account1,store_account2
SUPERMARKET_TARGETS=store_account1,store_account2

# Other settings
LOG_LEVEL=INFO
WORKER_INTERVAL_SECONDS=3600
